<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Help" />
		<Option pch_mode="0" />
		<Option default_target="help_plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="bzip2">
				<Option output="bzip2/libbzip2.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="../../../.objs/plugins/contrib/help_plugin/bzip2" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-mthreads" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Target title="zlib">
				<Option output="zlib/libz.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="../../../.objs/plugins/contrib/help_plugin/zlib" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-mthreads" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Target title="help_plugin">
				<Option output="../../../devel/share/CodeBlocks/plugins/help_plugin.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs/plugins/contrib/help_plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="-Winvalid-pch" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="bzip2" />
					<Add directory="zlib" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="bzip2/libbzip2.a" />
					<Add library="zlib/libz.a" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel\share\CodeBlocks\help_plugin.zip manifest.xml *.xrc *.png" />
					<Add after="cmd /c if not exist ..\..\..\devel\share\CodeBlocks\docs mkdir ..\..\..\devel\share\CodeBlocks\docs" />
					<Add after="xcopy /Y index.ini ..\..\..\devel\share\CodeBlocks\docs" />
					<Add after="cmd /c if not exist ..\..\..\output\share\CodeBlocks\docs mkdir ..\..\..\output\share\CodeBlocks\docs" />
					<Add after="xcopy /Y index.ini ..\..\..\output\share\CodeBlocks\docs" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="bzip2;zlib;help_plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=gnu++11" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#WX.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="../../../include/scripting/bindings" />
			<Add directory="../../../include/scripting/include" />
			<Add directory="../../../include/scripting/sqplus" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--add-stdcall-alias" />
			<Add option="-Wl,--enable-auto-import" />
			<Add library="wxmsw28$(WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="../../../devel" />
			<Add directory="$(#WX.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
